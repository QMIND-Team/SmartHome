import csv
"""
create a new csv file that take data from web_data.csv and 
for every 2 lines get temp data and find average between the 2 increment by 1 starting at 0
write a line in bewteen every 2 increment by 2 starting at 1
"""


def getList():
    web_data_list = []
    with open('web_data', 'rb') as f:
        csv_reader = csv.reader(f)
        web_data_list = list(csv_reader)
        return web_data_list

if __name__ == "__main__":
    print(getList())
